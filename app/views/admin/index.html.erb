<div class="container">
  <div class="card text-center">
    <div class="card-header">
      <ul class="nav nav-tabs card-header-tabs">
        <li class="nav-item">
          <a class="nav-link active" href="#">Manage unTilt Application</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/admin/users">Manage Interviewers</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/admin/interviews">Manage Interviews</a>
        </li>
        <li class="nav-item">
          <a class="nav-link active" href="/admin/candidates">Manage Candidates</a>
        </li>

      </ul>
    </div

<div class="card-body">
  <h2 class="card-title">Welcome Admin, <%= current_user.first_name %></h2>
  <h4> Here are all the current competencies: </h4>
  <ul>
    <% @competencies.each do |competency| %>
      <li> <%=competency.name%> </li>
      <li> <%=competency.description%> </li> <br>
      <%= link_to "Edit an Competency", edit_admin_competency_path(competency) %>
    <%end%>
      </ul>
  <%= button_to "Create a New Competency", new_admin_competency_path, method: :get, form_class: "btn btn-info"%>

  <%= button_to "Logout", logout_path, method: :delete, data: { confirm: "Are you sure?" }, form_class: "btn btn-info"%>


</div>
